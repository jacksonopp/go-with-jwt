package auth

import "go-with-jwt/cmd/web"

templ Login() {
  @web.Base() {
    <form hx-post="/login">
      <label>
        Email
        <input name="email" type="text" />
      </label>
      <hr />
      <label>
        Password
        <input name="password" type="password" />
      </label>
      <button type="submit">Log in</button>
    </form>
  }
}